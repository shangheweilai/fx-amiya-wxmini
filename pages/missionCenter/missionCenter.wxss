/* pages/missionCenter/missionCenter.wxss */
.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #fff;
    width: 100%;
    height: 100%;
}
.circle{
    width: 400rpx;
    height: 400rpx;
    border-radius: 200rpx;
    margin-top: 30rpx;
    background-color: orange;
    display: flex;
    justify-content: center;
    align-items: center;
}
.circle view{
    text-align: center;
}
.growth{
    margin-top: 30rpx;
    margin-bottom: 30rpx;
}